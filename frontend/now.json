{
  "version": 2,
  "name": "build",
  "alias": ["www.spoti-vote.com"],
  "routes": [
    {
      "src": "/(.*)",
      "status": 301,
      "headers": { "Location": "https://spoti-vote.com/$1" }
  },{
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {"Cache-Control": "max-age=60, s-maxage=86400" }
  },{
      "src": "/logos/(.*)",
      "dest": "/logos/$1",
      "headers": {"Cache-Control": "max-age=60, s-maxage=86400" }
  },{
      "src": "/favicon.ico",
      "dest": "/favicon.ico",
      "headers": {"Cache-Control": "max-age=60, s-maxage=86400" }
  },{
      "src": "/manifest.json",
      "dest": "/manifest.json",
      "headers": {"Cache-Control": "max-age=60, s-maxage=86400" }
  },{
      "src": "/service-worker.js",
      "dest": "/service-worker.js",
      "headers": {"Cache-Control": "no-cache" }
  }
  ]
}
