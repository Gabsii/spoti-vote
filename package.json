{
  "name": "spoti-vote",
  "version": "1.0.1",
  "description": "Vote for Spotify Queue",
  "author": "Gabsii",
  "license": "Apache-2.0",
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"npm:start-frontend\" \"npm:start-backend\"",
    "start-frontend": "node frontend/scripts/start.js",
    "start-backend": "dotenv -- cross-var vercel dev ./backend -l %PORTBACK%",
    "build": "node frontend/scripts/build.js",
    "deploy": "now --prod",
    "test": "node frontend/scripts/test.js",
    "deploy-backend": "pm2 delete backendSpotivote; pm2 start backend/server.js --name \"backendSpotivote\""
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gabsii/spoti-vote.git"
  },
  "keywords": [
    "spotify",
    "vote",
    "nodejs",
    "reactjs"
  ],
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "bugs": {
    "url": "https://github.com/Gabsii/spoti-vote/issues"
  },
  "homepage": "https://spoti-vote.com",
  "jest": {
    "projects": [
      {
        "displayName": "other-lint",
        "runner": "jest-runner-eslint",
        "testMatch": [
          "<rootDir>/scripts/**/*.js"
        ]
      },
      {
        "displayName": "backend-lint",
        "runner": "jest-runner-eslint",
        "testMatch": [
          "<rootDir>/backend/**/*.js"
        ]
      },
      {
        "displayName": "backend",
        "testMatch": [
          "<rootDir>/backend/**/__tests__/**/*test.{js,jsx,mjs}"
        ]
      },
      {
        "displayName": "frontend-lint",
        "runner": "jest-runner-eslint",
        "testMatch": [
          "<rootDir>/frontend/src/**/*.{js,jsx}"
        ]
      },
      {
        "displayName": "frontend testing",
        "roots": [
          "<rootDir>/frontend/src"
        ],
        "collectCoverageFrom": [
          "frontend/src/**/*.{js,jsx,ts,tsx}",
          "!frontend/src/**/*.d.ts"
        ],
        "setupFiles": [
          "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [],
        "testMatch": [
          "<rootDir>/frontend/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
          "<rootDir>/frontend/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jest-environment-jsdom-fourteen",
        "transform": {
          "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
          "^.+\\.css$": "<rootDir>/frontend/config/jest/cssTransform.js",
          "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/frontend/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
          "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
          "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
          "^react-native$": "react-native-web",
          "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
          "web.js",
          "js",
          "web.ts",
          "ts",
          "web.tsx",
          "tsx",
          "json",
          "web.jsx",
          "jsx",
          "node"
        ]
      }
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.31",
    "@fortawesome/free-brands-svg-icons": "^5.15.0",
    "@fortawesome/free-solid-svg-icons": "^5.15.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "autoprefixer": "^9.8.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chalk": "^2.4.2",
    "create-react-app": "^3.4.1",
    "cross-var": "^1.1.0",
    "css-loader": "^3.6.0",
    "dotenv": "^8.0.0",
    "dotenv-cli": "^4.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^6.5.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^1.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.3.0",
    "fs-extra": "^8.0.1",
    "glamor": "^2.20.40",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fourteen": "^0.1.0",
    "jest-runner-eslint": "^0.7.7",
    "jest-watch-typeahead": "^0.4.2",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^0.8.2",
    "node-fetch": "^2.6.1",
    "object-assign": "^4.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "promise": "^8.1.0",
    "raf": "^3.4.1",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^9.1.0",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-lazyload": "^2.6.9",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-text-marquee": "^1.0.1",
    "react-web-notification": "^0.7.0",
    "request": "^2.88.2",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "sweetalert2": "^8.19.0",
    "terser-webpack-plugin": "^2.3.8",
    "universal-cookie": "^4.0.4",
    "url-loader": "^2.3.0",
    "webpack": "^4.44.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.0.4",
    "whatwg-fetch": "^3.4.1",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "speed-measure-webpack-plugin": "^1.3.3",
    "webpack-bundle-analyzer": "^3.9.0"
  }
}
