{
  "name": "spoti-vote",
  "version": "1.0.1",
  "description": "Vote for Spotify Queue",
  "scripts": {
    "install-ui": "cd frontend && npm install",
    "install-backend": "cd backend && npm install",
    "install": "npm run install-ui && npm run install-backend",
    "start": "concurrently --kill-others-on-fail \"npm run start-ui\" \"npm run start-backend\"",
    "start-ui": "cd frontend && npm start",
    "start-backend": "cd backend && nodemon server.js",
    "test-ui": "cd frontend && npm test",
    "test-backend": "cd backend && npm test"
  },
  "cacheDirectories": [
    "node_modules",
    "spoti-vote-backend/node_modules",
    "spoti-vote/node_modules"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gabsii/spoti-vote.git"
  },
  "keywords": [
    "spotify",
    "vote",
    "nodejs",
    "reactjs"
  ],
  "author": "Gabsii",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Gabsii/spoti-vote/issues"
  },
  "homepage": "https://github.com/Gabsii/spoti-vote#readme"
}
