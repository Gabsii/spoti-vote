{
  "name": "spoti-vote",
  "version": "1.0.0",
  "description": "Vote for Spotify Queue",
  "author": "Gabsii",
  "license": "Apache-2.0",
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"npm:start-frontend\" \"npm:start-backend\"",
    "start-frontend": "node frontend/scripts/start.js",
    "start-backend": "nodemon ./backend/server.js",
    "build": "node frontend/scripts/build.js",
    "deploy": "npm run build && now --public ./frontend/build && now alias",
    "test": "node scripts/test.js --env=jsdom",
    "update": "npm-update-all"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gabsii/spoti-vote.git"
  },
  "keywords": [
    "spotify",
    "vote",
    "nodejs",
    "reactjs"
  ],
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "bugs": {
    "url": "https://github.com/Gabsii/spoti-vote/issues"
  },
  "now": {
    "name": "build",
    "alias": [
      "www.spoti-vote.com",
      "spoti-vote.com"
    ]
  },
  "homepage": "https://spoti-vote.com",
  "jest": {
    "watchPlugins": [
      "jest-runner-eslint/watch-fix"
    ],
    "projects": [
      {
        "displayName": "backend",
        "testMatch": [
          "<rootDir>/backend/**/__tests__/**/*.{js,jsx,mjs}"
        ]
      },
      {
        "runner": "jest-runner-eslint",
        "displayName": "backend-lint",
        "testMatch": [
          "<rootDir>/backend/**/*.js"
        ]
      },
      {
        "runner": "jest-runner-eslint",
        "displayName": "frontend-lint",
        "testMatch": [
          "<rootDir>/frontend/src/**/*.{js,jsx}"
        ]
      },
      {
        "displayName": "frontend",
        "collectCoverageFrom": [
          "/frontend/src/**/*.{js,jsx,mjs}"
        ],
        "setupFiles": [
          "<rootDir>/frontend/config/polyfills.js"
        ],
        "testMatch": [
          "<rootDir>/frontend/src/**/__tests__/**/*.{js,jsx,mjs}",
          "<rootDir>/frontend/src/**/?(*.)(spec|test).{js,jsx,mjs}"
        ],
        "testPathIgnorePatterns": [
          "<rootDir>/(build|docs|node_modules)/"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
          "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
          "^.+\\.css$": "<rootDir>/frontend/config/jest/cssTransform.js",
          "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/frontend/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
          "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
        ],
        "moduleNameMapper": {
          "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
          "web.js",
          "mjs",
          "js",
          "json",
          "web.jsx",
          "jsx",
          "node"
        ]
      }
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "acorn": "^6.1.1",
    "acorn-dynamic-import": "^4.0.0",
    "autoprefixer": "^9.6.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.0.0",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "chalk": "^2.4.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "create-react-app": "^3.0.1",
    "css-loader": "^3.0.0",
    "deep-equal": "^1.0.1",
    "dotenv": "^8.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.0-next.74",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.11.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.0.0",
    "fs-extra": "^8.0.1",
    "glamor": "^2.20.40",
    "helmet": "^3.18.0",
    "helmet-csp": "^2.7.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "image-webpack-loader": "^5.0.0",
    "ip": "^1.1.5",
    "jest": "^24.8.0",
    "jest-runner-eslint": "^0.7.4",
    "lodash": "^4.17.11",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.19.1",
    "npm-update-all": "^1.0.1",
    "object-assign": "^4.1.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "preact": "^8.4.2",
    "preact-compat": "^3.19.0",
    "promise": "^8.0.3",
    "querystring": "^0.2.0",
    "raf": "^3.4.1",
    "react": "^16.8.6",
    "react-cookie": "^4.0.0",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.6",
    "react-lazyload": "^2.6.1",
    "react-loadable": "^5.5.0",
    "react-responsive": "^7.0.0",
    "react-router-dom": "^5.0.1",
    "react-text-marquee": "^1.0.1",
    "react-web-notification": "^0.6.0",
    "really-smooth-scroll": "^2.1.1",
    "request": "^2.88.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2",
    "sw-precache-webpack-plugin": "^0.11.5",
    "sweetalert2": "^8.13.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.0.1",
    "webpack": "^4.39.2",
    "webpack-dev-server": "^3.8.0",
    "webpack-manifest-plugin": "^2.0.4",
    "whatwg-fetch": "^3.0.0",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "devDependencies": {
    "speed-measure-webpack-plugin": "^1.3.1",
    "terser-webpack-plugin": "^1.3.0"
  }
}
